{
  "name": "threeflows",
  "version": "0.1.0",
  "main": "bundle.js",
  "scripts": {
    "build": "cp *.html build && browserify -t [babelify] src/main.jsx -o build/bundle.js",
    "watch": "npm run build && watchify -t [babelify] -p livereactload src/main.jsx -o build/bundle.js",
    "flow-quiet": "flow; test $? -eq 0 -o $? -eq 2",
    "lint-quiet": "eslint --fix --ext jsx --ext js -c .eslintrc src; exit 0",
    "lint": "eslint --ext jsx --ext js -c .eslintrc src",
    "test": "npm run lint && jest && flow",
    "jest-quiet": "jest; exit 0"
  },
  "jest": {
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "./node_modules/react/",
      "./node_modules/material-ui/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mit-teaching-systems-lab/threeflows.git"
  },
  "author": "krob@mit.edu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mit-teaching-systems-lab/threeflows/issues"
  },
  "homepage": "https://github.com/mit-teaching-systems-lab/threeflows#readme",
  "dependencies": {
    "appearin-sdk": "0.0.4",
    "crossfilter": "^1.3.12",
    "d3": "^3.5.17",
    "date-fns": "^1.3.0",
    "dc": "^2.0.0-beta.30",
    "lodash": "^4.13.1",
    "material-ui": "^0.15.0",
    "node-uuid": "^1.4.7",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-mini-router": "^2.0.0",
    "react-motion": "^0.4.4",
    "react-swipeable-views": "^0.6.3",
    "react-tap-event-plugin": "^1.0.0",
    "rollbar-browser": "^1.9.1",
    "superagent": "^2.0.0",
    "superagent-promise": "^1.1.0",
    "velocity-animate": "^1.2.3",
    "velocity-react": "^1.1.5",
    "watchify": "3.7.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-jest": "^13.2.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "eslint": "^2.13.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "flow-bin": "^0.27.0",
    "jest-cli": "^13.2.3",
    "livereactload": "^2.2.3",
    "react-addons-test-utils": "^15.2.1",
    "react-proxy": "^1.1.8"
  }
}
